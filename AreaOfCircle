#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double radius, area;
    
    cout << "Enter a Radius: ";
    cin >> radius;
    
    if (radius >= 0) {
        area = 3.14 * pow(radius, 2);
        
        cout << "Area of Circle with Radius " << radius << " is " << area;
    }
    
    else {
        cout << "Invalid!";
    }

    return 0;
}
